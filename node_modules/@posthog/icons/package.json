{
    "name": "@posthog/icons",
    "version": "0.36.0",
    "author": "PostHog <hey@posthog.com>",
    "repository": "https://github.com/PostHog/icons",
    "homepage": "https://posthog-icons.vercel.app",
    "license": "MIT",
    "main": "./dist/posthog-icons.cjs.js",
    "module": "./dist/posthog-icons.es.js",
    "types": "./dist/index.d.ts",
    "exports": {
        ".": {
            "types": "./dist/index.d.ts",
            "import": "./dist/posthog-icons.es.js",
            "require": "./dist/posthog-icons.cjs.js"
        }
    },
    "files": [
        "dist/"
    ],
    "scripts": {
        "dev": "vite",
        "build": "vite build",
        "build:demo": "vite build --config vite.demo.config.ts",
        "lint": "eslint src --ext ts,tsx --report-unused-disable-directives",
        "preview": "vite preview",
        "prepublishOnly": "yarn lint && yarn build",
        "prepare": "husky",
        "prettier": "prettier -w .",
        "setup": "yarn install && yarn build"
    },
    "peerDependencies": {
        "react": ">=16.14.0",
        "react-dom": ">=16.14.0"
    },
    "devDependencies": {
        "@types/react": "^18.2.14",
        "@types/react-dom": "^18.2.6",
        "@typescript-eslint/eslint-plugin": "^5.61.0",
        "@typescript-eslint/parser": "^5.61.0",
        "@vitejs/plugin-react": "^4.0.1",
        "eslint": "^8.44.0",
        "eslint-plugin-react-hooks": "^4.6.0",
        "eslint-plugin-react-refresh": "^0.4.1",
        "eslint-plugin-svg-jsx": "^1.2.4",
        "husky": "^9.1.6",
        "lint-staged": "^15.2.10",
        "prettier": "^3.3.3",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "typescript": "^5.0.2",
        "vite": "^4.4.0",
        "vite-plugin-dts": "^3.2.0"
    },
    "lint-staged": {
        "*.{ts,tsx}": [
            "eslint --fix",
            "prettier --write"
        ],
        "*.{json,yaml,yml}": "prettier --write"
    },
    "packageManager": "yarn@1.22.19+sha512.ff4579ab459bb25aa7c0ff75b62acebe576f6084b36aa842971cf250a5d8c6cd3bc9420b22ce63c7f93a0857bc6ef29291db39c3e7a23aab5adfd5a4dd6c5d71"
}
